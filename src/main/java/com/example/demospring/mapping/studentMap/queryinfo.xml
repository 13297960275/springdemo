<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demospring.mapping.studentMap.queryinfo">
    <select id="query" resultType="com.example.demospring.entity.student">
        select * from test;
    </select>
    <insert id="insert">
        insert test (id,name) values (#{id},#{name})
    </insert>
</mapper>